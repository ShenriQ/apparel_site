{type : API_GETFAV, payload : {}});\r\n      yield put({type : SHOW_ALERT, payload : {type : 'success', msg : 'The product is added to your favorites!'}});\r\n    }\r\n    else if(action.type == API_GETFAV)\r\n    {\r\n      let user =  getUser()\r\n      if (user == null || user.token == null)\r\n      {\r\n        yield put({type : SET_FAVS, payload : []});\r\n      }\r\n      else\r\n      {\r\n        const payload = yield call(getAllFavs, {user_id : user.token});\r\n        yield put({type : SET_FAVS, payload : payload});\r\n      }\r\n    }\r\n\r\n  } catch (e) {\r\n    console.log(e)\r\n    yield put({ type: DISMISS_LOAD, payload: '' });\r\n    if(e.response != null && e.response.data != null && e.response.data.message != null)\r\n    {\r\n      yield put({type : SHOW_ALERT, payload : {type : 'error', msg : e.response.data.message }});\r\n    }\r\n    \r\n  }\r\n}\r\n","F:\\3.reactjs_workspace\\2.hk_ecommerce\\hk_store_react\\react_eshop\\src\\redux_helper\\sagas\\index.js",["599","600"],"import { takeEvery, call, put, all } from \"redux-saga/effects\";\r\nimport fav from './fav';\r\nimport user from './user';\r\n\r\nexport default function* watcherSaga() {\r\n  yield all([call(fav), call(user)]);\r\n}\r\n","F:\\3.reactjs_workspace\\2.hk_ecommerce\\hk_store_react\\react_eshop\\src\\redux_helper\\sagas\\user.js",["601","602","603","604"],"import { takeEvery,takeLatest, call, put } from \"redux-saga/effects\";\r\nimport {API_LOGIN, API_REGISTER, API_GETFAV, GET_CART, SHOW_LOAD, DISMISS_LOAD, SHOW_ALERT, SET_USER} from '../constants/action-types';\r\nimport {doLogin, doRegister} from '../../apis/user';\r\nimport _localstorage from '../../utils/localstorage';\r\n\r\nexport default function* watcherSaga() {\r\n  yield takeEvery([API_LOGIN, API_REGISTER], workerSaga);\r\n}\r\n\r\nfunction* workerSaga(action) {\r\n  try {\r\n    if(action.type == API_LOGIN)\r\n    {\r\n      yield put({type : SHOW_LOAD, payload : 'Loging in...'});\r\n      const res = yield call(doLogin, action.payload.user);\r\n      yield put({type : SET_USER, payload : {token : res.user_id, name : res.user_name, photo : res.user_profile_photo}});\r\n      action.payload.callback('success')\r\n      yield put({type : SHOW_LOAD, payload : 'Loading...'});\r\n      yield put({type : GET_CART, payload : ''});\r\n      yield put({type : API_GETFAV, payload : ''});\r\n      yield put({type : DISMISS_LOAD, payload : ''});\r\n      yield put({type : SHOW_ALERT, payload : {type : 'success', msg : 'Login Success!'}});\r\n    }\r\n    else if(action.type == API_REGISTER)\r\n    {\r\n      yield put({type : SHOW_LOAD, payload : 'Registering ...'});\r\n      const res = yield call(doRegister, action.payload.user);\r\n      yield put({type : SET_USER, payload : {token : res.user_id, name : res.user_name, photo : res.user_profile_photo}});\r\n      action.payload.callback('success')\r\n      yield put({type : SHOW_LOAD, payload : 'Loading...'});\r\n      yield put({type : GET_CART, payload : ''});\r\n      yield put({type : API_GETFAV, payload : ''});\r\n      yield put({type : DISMISS_LOAD, payload : ''});\r\n      yield put({type : SHOW_ALERT, payload : {type : 'success', msg : 'Register Success!'}});\r\n    }\r\n\r\n  } catch (e) {\r\n    console.log(e)\r\n    yield put({ type: DISMISS_LOAD, payload: '' });\r\n    if(e.response != null && e.response.data != null && e.response.data.message != null)\r\n    {\r\n      yield put({type : SHOW_ALERT, payload : {type : 'error', msg : e.response.data.message }});\r\n    }\r\n  }\r\n}\r\n","F:\\3.reactjs_workspace\\2.hk_ecommerce\\hk_store_react\\react_eshop\\src\\components\\shop\\Bread\\index.js",["605","606","607","608"],"F:\\3.reactjs_workspace\\3.apparel\\devel\\web\\apparel_site\\src\\index.js",[],["609","610"],"F:\\3.reactjs_workspace\\3.apparel\\devel\\web\\apparel_site\\src\\App.js",["611"],"F:\\3.reactjs_workspace\\3.apparel\\devel\\web\\apparel_site\\src\\components\\modals\\loading.js",["612"],"F:\\3.reactjs_workspac